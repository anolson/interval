<html>
  <head>
    <title>Interval</title>
    <%= stylesheet_link_tag 'style' %>
    <%= javascript_include_tag :defaults %>
    <%= javascript_include_tag "lightbox" %>
    <script language="javascript" type="text/javascript" src="/javascripts/flotr/flotr.debug-0.1.0alpha.js"></script>
  </head>
  <body>

    <div id="title">
      <%= link_to image_tag("interval.png"),  '/'  %>
    </div>
   
    <%= render(:partial => 'common/navigation') %>
    
    <div id="content">
      <div class="container full">
        <div id="sub">
          <%= @primary_heading %>
          <div class="smaller edit">
            <%= @editable && render(:partial => 'workouts/edit_delete', :locals => {:id => @workout.id}) || nil %>
          </div>
          <div class="clear"></div>
        </div>
        <%= @secondary_heading && render(:partial => 'common/secondary') %>
        <%= @content_for_layout  %>
        <div class="clear"></div>
      </div>
    </div>
    <%= render :partial => 'common/footer' %>
  </body>
</html>