<% @primary_heading= link_to content_tag(:h1, "Training Summary"), :action => "index" %>

<div class="summary">
  <% rounded_box "Weekly", "half", "full" do %>
    <% container "full" do %>
      <%= render :partial => 'weekly' %>
    <% end %>
  <% end %>

  <% rounded_box "Monthly", "half", "full" do %>
    <% container "full" do %>
      <%= render :partial => 'monthly' %>
    <% end %>
  <% end %>
</div>