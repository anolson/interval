<% unless @recent_workouts.empty? && @processors.empty? %>
  <% rounded_box nil, "complete", "wide" do %>
    <div class="top">
      Recent Workouts
    </div>
    <ul id="recent">
      <%= render :partial => "common/workouts/recent_workout", :collection => @recent_workouts  %>
    </ul>
    <%= render :partial => "processors/processors", :locals => { :processors => @processors } %>
  <% end %>
<% end %>

