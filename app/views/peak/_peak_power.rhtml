  <tr class="<%= cycle('even', 'odd', :name => 'class') -%>">
    <td align="right"><%= duration_in_words peak_power[:duration] %> :</td>
    <td>
      <%= peak_power[:value].round %> W 
      <%= peak_power[:value].round.eql?(@user.peak_powers.maximum_peak_power(peak_power[:duration])) && "*" || nil %>
    </td> 
  </tr>

