<% @primary_heading= link_to content_tag(:h1, "User Preferences"), :action => "index" %>

<div id="main">
  <% rounded_box nil, "complete", "half" do %>
    <% container "full" do %>
      <% form_tag do %>
        <%= render :partial => 'billing' %>

      <%= submit_tag "Update" %>
      <%= set_focus_for('credit_card') %>
      <% end %>
    <% end %>
  <% end %>
</div>
